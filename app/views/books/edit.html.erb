<h1>Editing Book</h1>
  <div class="form">
    <%= form_with model: @book, url: book_path(@book.id), method: :patch do |f| %>
<h4>Title</h4>
  <div class="field">
    <%= f.text_field :title %>
  </div>
<h4>Body</h4>
  <div class="field">
    <%= f.text_area :body %>
  </div>
  <div class="button">
    <%= f.submit 'Update Book' %>
   <% end %>
  </div>

 <%= link_to "Show", book_path(@book.id) %> | <%= link_to "Back", books_path %>